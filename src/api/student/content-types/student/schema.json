{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "uid": {
      "type": "uid",
      "required": true
    },
    "classes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::class.class",
      "inversedBy": "students"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "undefined",
        "male",
        "female"
      ],
      "default": "undefined"
    },
    "complete_sessions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::session.session",
      "mappedBy": "complete_students"
    }
  }
}
